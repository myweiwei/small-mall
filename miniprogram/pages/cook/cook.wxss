.catalog-name{
  margin-top: 20rpx;
  margin-left: 25rpx;
}
.product{
  width: 100%;
  display: flex;
  flex-wrap: wrap;

}
.product-child{
  width: calc(calc(100% / 4) - 30rpx);
  height:150rpx;
  margin-left: 25rpx;
  margin-top: 25rpx;
  box-sizing: border-box;
  background: #FFF;
  display: flex;
  flex-direction: column;
}
.product-image{
  width: 100%;
  height: 100%;
}


.imageClass{
  background:linear-gradient(#f00, #00f);
  background-blend-mode: lighten;
}
